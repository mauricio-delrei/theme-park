<%- include('header') %>

<main>
  <h1>Events</h1>
  <ul>
    <% events.forEach(event => { %>
      <li>
        <h2><%= event.title %></h2>
        <p><%= event.description %></p>
        <p>Date: <%= event.date %></p>
        <p>Status: <%= event.is_past ? 'Past' : 'Upcoming' %></p>
      </li>
    <% }); %>
  </ul>
</main>

<%- include('footer') %>
